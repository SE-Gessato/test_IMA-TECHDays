s DO\n\t\t\tGVL_MulticarrierEmulation.G_audiStationId[udiLoop]:= udiLoop;\n\t\tEND_FOR\n\n//--- Carriers\n\t//--- Set Track-ID\n\t\tFOR udiLoop := 1 TO GVL_MulticarrierConfiguration.Gc_udiNumberOfCarriers DO\n\t\t\tGVL_MulticarrierEmulation.G_audiCarriersTrackID[udiLoop]:= 1;\n\t\tEND_FOR\n\n\t//--- Set Color\n\t\tFOR udiLoop := 1 TO GVL_MulticarrierConfiguration.Gc_udiNumberOfCarriers DO\n\t\t\tIF udiLoop = 1 THEN\n\t\t\t\tGVL_MulticarrierEmulation.G_aetCarrierColor[udiLoop] := DTC.ET_EmulationColors.LimeGreen;\n\t\t\tELSE\n\t\t\t\tGVL_MulticarrierEmulation.G_aetCarrierColor[udiLoop] := DTC.ET_EmulationColors.None;\n\t\t\tEND_IF\n\t\tEND_FOR\n\n\t//--- Set Tool Family\n\t\tFOR udiLoop := 1 TO GVL_MulticarrierConfiguration.Gc_udiNumberOfCarriers DO\n\t\t\tGVL_MulticarrierEmulation.G_asToolFamily[udiLoop]:= 'StandardTool';\n\t\tEND_FOR\n\n\t//--- Set Tool Dimensions\n\t\tFOR udiLoop := 1 TO GVL_MulticarrierConfiguration.Gc_udiNumberOfCarriers DO\n\t\t\tGVL_MulticarrierEmulation.G_astToolDimensions[udiLoop]:= GVL_MulticarrierEmulation.G_stDefaultDimensions;\n\t\tEND_FOR\n\n\t//--- Set Tool Offset\n\t\tFOR udiLoop := 1 TO GVL_MulticarrierConfiguration.Gc_udiNumberOfCarriers DO\n\t\t\tGVL_MulticarrierEmulation.G_astToolOffsets[udiLoop]:= GVL_MulticarrierEmulation.G_stDefaultVector3D;\n\t\tEND_FOR\n\n\t//--- Set Tool Color\n\t\tFOR udiLoop := 1 TO GVL_MulticarrierConfiguration.Gc_udiNumberOfCarriers DO\n\t\t\tGVL_MulticarrierEmulation.G_aetToolColor[udiLoop]:= DTC.ET_EmulationColors.DarkSlateGray;\n\t\tEND_FOR\n\n\t//--- Set Product Family\n\t\tFOR udiLoop := 1 TO GVL_MulticarrierConfiguration.Gc_udiNumberOfCarriers DO\n\t\t\tGVL_MulticarrierEmulation.G_asProductFamily[udiLoop]:= 'StandardProduct';\n\t\tEND_FOR\n\n\t//--- Set Product Dimensions\n\t\tFOR udiLoop := 1 TO GVL_MulticarrierConfiguration.Gc_udiNumberOfCarriers DO\n\t\t\tGVL_MulticarrierEmulation.G_astProductDimensions[udiLoop]:= GVL_MulticarrierEmulation.G_stDefaultDimensions;\n\t\tEND_FOR\n\n\t//--- Set Product Offset\n\t\tFOR udiLoop := 1 TO GVL_MulticarrierConfiguration.Gc_udiNumberOfCarriers DO\n\t\t\tGVL_MulticarrierEmulation.G_astProductOffsets[udiLoop]:= GVL_MulticarrierEmulation.G_stDefaultVector3D;\n\t\tEND_FOR\n\n\t//--- Set Product Color\n\t\tFOR udiLoop := 1 TO GVL_MulticarrierConfiguration.Gc_udiNumberOfCarriers DO\n\t\t\tGVL_MulticarrierEmulation.G_aetProductColor[udiLoop]:= DTC.ET_EmulationColors.Transparent;\n\t\tEND_FOR\n\nxInitMulticarrierEmulationDone := TRUE;","LineInfoPersistence":"(string)d18a00ee-b7cc-429d-b675-4ec42d1945ed_Impl_LineIds"}}}},"Interface":{"@Type":"a9ed5b7e","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)METHOD Init_MulticarrierEmulation\nVAR\nEND_VAR\n","LineInfoPersistence":"(string)d18a00ee-b7cc-429d-b675-4ec42d1945ed_Decl_LineIds"}}}}}},"TypeInfos":{"3b83b776":"{3b83b776-fb25-43b8-99f2-3c507c9143fc}","a9ed5b7e":"{a9ed5b7e-75c5-4651-af16-d2c27e98cb94}","f3878285":"{f3878285-8e4f-490b-bb1b-9acbb7eb04db}","f8a58466":"{f8a58466-d7f6-439f-bbb8-d4600e41d099}","string":"System.String"}}},"FormatVersion":"1.0"}