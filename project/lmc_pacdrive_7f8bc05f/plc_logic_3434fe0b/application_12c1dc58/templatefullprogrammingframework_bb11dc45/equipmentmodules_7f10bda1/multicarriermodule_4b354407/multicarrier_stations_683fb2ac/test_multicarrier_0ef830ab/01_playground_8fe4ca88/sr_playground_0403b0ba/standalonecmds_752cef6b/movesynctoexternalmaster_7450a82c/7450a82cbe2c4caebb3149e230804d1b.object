{"payload":{"meta":{"Graph":{"@Type":"81297157","@Value":{"Guid":"(Guid)7450a82c-be2c-4cae-bb31-49e230804d1b","ParentGuid":"(Guid)0403b0ba-733f-43db-97c2-48be15d20a04","Name":"(string)MoveSyncToExternalMaster","Properties":{"@Type":"2c41fa04:IDictionary","@Value":[{"@Key":"(Guid)829a18f2-c514-4f6e-9634-1df173429203","@Value":{"@Type":"829a18f2","@Value":{"ParentObjects":{"@Type":"fa2ee218:IDictionary","@Value":[{"@Key":"(Guid)d9b2b2cc-ea99-4c3b-aa42-1e5c49e65b84","@Value":"(Guid)752cef6b-1fef-4c6c-9b2b-37fba8b355c0"}]}}}}]},"TypeGuid":"(Guid)8ac092e5-3128-4e26-9e7e-11016c6684f2","EmbeddedTypeGuids":{"@Type":"[Guid]","@Value":["(Guid)3b83b776-fb25-43b8-99f2-3c507c9143fc"]}}},"TypeInfos":{"2c41fa04":"{2c41fa04-1834-41c1-816e-303c7aa2c05b}","81297157":"{81297157-7ec9-45ce-845e-84cab2b88ade}","829a18f2":"{829a18f2-c514-4f6e-9634-1df173429203}","fa2ee218":"{fa2ee218-a39b-4b6d-b249-49dbddbd168a}","Guid":"System.Guid","string":"System.String"}},"object":{"Graph":{"@Type":"8ac092e5","@Value":{"Implementation":{"@Type":"3b83b776","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)IF \txMoveSyncToExternalMaster AND \n\tudiCarrierIndexMoveSyncExternal > 0 AND \n\tudiCarrierIndexMoveSyncExternal <= i_ifMulticarrier.ifFeedback.ifFeedbackConfiguration.udiNumberOfCarriers \nTHEN\n\txMoveSyncToExternalMaster := FALSE;\n\n\tIF xUseDRV_Master THEN\n\t\tifExternalMaster := DRV_Master;\n\tELSE\n\t\tifExternalMaster := DRV_Slave1;\n\tEND_IF\n\t\n\t//--- Load Motion Parameter in case of Carrier is stopped\n\ti_ifMulticarrier.raifCarrier[udiCarrierIndexMoveSyncExternal].ifMotion.SetMotionParameter(\n\t\t\t\t\ti_lrMaxVelocity\t\t\t:= 500, \n\t\t\t\t\ti_lrMaxAcceleration\t\t:= lrSingleStopDecPlayGround, \n\t\t\t\t\ti_lrMaxDeceleration\t\t:= lrSingleStopDecPlayGround, \n\t\t\t\t\ti_lrMaxAbsJerk\t\t\t:= lrSingleStopJerkPlayGround,\n\t\t\t\t\tq_xError\t\t\t\t=> xError,\n\t\t\t\t\tq_etResult\t\t\t\t=> etResultMCR,\n\t\t\t\t\tq_sResultMsg\t\t\t=> sResultMsg);\t\n\t\n\tIF NOT xError THEN\n\t\ti_ifMulticarrier.raifCarrier[udiCarrierIndexMoveSyncExternal].ifMotion.ifMoveSyncFromStandstill.StartSyncToExternalMaster(\n\t\t\t\t\ti_ifMaster\t\t\t\t\t:= ifExternalMaster, \n\t\t\t\t\ti_etMasterSetposMode\t\t:= etMasterSetposMode, \n\t\t\t\t\ti_lrMasterSetposValue\t\t:= lrMasterSetposValue, \n\t\t\t\t\ti_lrXStartPhasingToMaster\t:= lrXStartPhasingToMaster, \n\t\t\t\t\ti_lrXEndPhasingToMaster\t\t:= lrXEndPhasingToMaster,\n\t\t\t\t\ti_xMasterPositiveDirection\t:= xMasterPositiveDirection,\n\t\t\t\t\ti_xInvertedMovementToMaster\t:= xInvertedMovementToMaster, \n\t\t\t\t\tq_xError\t\t\t\t\t=> xError,\n\t\t\t\t\tq_etResult\t\t\t\t\t=> etResultMCR,\n\t\t\t\t\tq_sResultMsg\t\t\t\t=> sResultMsg);\n\tEND_IF\nEND_IF\n\n//--- Calculate delta up to carrier in snyc ---------------------------------------   \nIF i_ifMulticarrier.raifCarrier[udiCarrierIndexMoveSyncExternal].ifFeedback.ifFeedbackMoveSyncFromStandstill.etStateMoveSyncFromStandstill = MCR.ET_StateMoveSyncFromStandstill.NotActive THEN\n\tlrDeltaPosPhasingToMaster := 0.0; \n\txSnycPosSaved := FALSE; \n\nELSIF i_ifMulticarrier.raifCarrier[udiCarrierIndexMoveSyncExternal].ifFeedback.ifFeedbackMoveSyncFromStandstill.etStateMoveSyncFromStandstill = MCR.ET_StateMoveSyncFromStandstill.PhasingToMaster THEN\n\tIF NOT xSnycPosSaved THEN  \n\t\tlrSaveStartPhasingPos := i_ifMulticarrier.raifCarrier[udiCarrierIndexMoveSyncExternal].ifFeedback.ifFeedbackMotionValues.lrRefTrackPosition;\n\t\txSnycPosSaved := TRUE; \n\tEND_IF \n\tIF NOT xInvertedMovementToMaster THEN\n\t\tlrDeltaPosPhasingToMaster := i_ifMulticarrier.raifCarrier[udiCarrierIndexMoveSyncExternal].ifFeedback.ifFeedbackMotionValues.lrRefTrackPosition - lrSaveStartPhasingPos;\n\tELSE\n\t\tlrDeltaPosPhasingToMaster := lrSaveStartPhasingPos - i_ifMulticarrier.raifCarrier[udiCarrierIndexMoveSyncExternal].ifFeedback.ifFeedbackMotionValues.lrRefTrackPosition;\t\n\tEND_IF\n\txCarrierInSyncPhase := FALSE;\nELSIF i_ifMulticarrier.raifCarrier[udiCarrierIndexMoveSyncExternal].ifFeedback.ifFeedbackMoveSyncFromStandstill.etStateMoveSyncFromStandstill  = MCR.ET_StateMoveSyncFromStandstill.InPhaseToMaster AND NOT xCarrierInSyncPhase THEN\n\txCarrierInSyncPhase := TRUE; \nEND_IF\n\n\n\n//--- only for vis ---------------------------------------   \nCASE uiSelectMaster OF\n\n\t0:\tIF xUseDRV_Master THEN\n\t\t\txUseDRV_Slave1 := FALSE;\n\t\t\tuiSelectMaster := 1;\n\t\tEND_IF\n\t\tIF xUseDRV_Slave1 THEN\n\t\t\txUseDRV_Master := FALSE;\n\t\t\tuiSelectMaster := 2;\n\t\tEND_IF \t\n\t\n\t1: \tIF xUseDRV_Slave1 THEN\n\t\t\txUseDRV_Master := FALSE;\n\t\t\tuiSelectMaster := 2;\n\t\tELSIF NOT xUseDRV_Master THEN\n\t\t\txUseDRV_Slave1 := TRUE;\n\t\t\tuiSelectMaster := 2;\n\t\tEND_IF \t\n\t\t\n\t2: \tIF xUseDRV_Master THEN\n\t\t\txUseDRV_Slave1 := FALSE;\n\t\t\tuiSelectMaster := 1;\n\t\tELSIF NOT xUseDRV_Slave1 THEN\n\t\t\txUseDRV_Master := TRUE;\n\t\t\tuiSelectMaster := 1;\n\t\tEND_IF \t\n\nEND_CASE","LineInfoPersistence":"(string)7450a82c-be2c-4cae-bb31-49e230804d1b_Impl_LineIds"}}}},"UniqueIdGenerator":"(string)208"}},"TypeInfos":{"3b83b776":"{3b83b776-fb25-43b8-99f2-3c507c9143fc}","8ac092e5":"{8ac092e5-3128-4e26-9e7e-11016c6684f2}","f3878285":"{f3878285-8e4f-490b-bb1b-9acbb7eb04db}","string":"System.String"}}},"FormatVersion":"1.0"}