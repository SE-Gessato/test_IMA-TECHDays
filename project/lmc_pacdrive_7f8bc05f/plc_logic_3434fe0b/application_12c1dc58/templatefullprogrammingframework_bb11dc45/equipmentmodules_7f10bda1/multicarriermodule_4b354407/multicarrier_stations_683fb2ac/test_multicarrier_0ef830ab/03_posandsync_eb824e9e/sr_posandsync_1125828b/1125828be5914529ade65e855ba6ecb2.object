);\t\t\t\n\t\t\n\t\tfbUnloadStation.CyclicMotionCall(iq_xTrigger := xStartUnload);\t\t\n\tEND_IF\n\t\t\nEND_CASE\n\n//--- set ApplicationLogger entries\nfbLoadStation.xEnableActivationOfAPLEntriesForCarriers   \t:= GVL_Project.G_xEnableAplEntriesViaStation;\nfbLoadStation.xActivateAPLEntriesForMovedCarriers \t\t\t:= xApplicationLoggerEntriesActiveLoad;\nfbFillStation.xEnableActivationOfAPLEntriesForCarriers   \t:= GVL_Project.G_xEnableAplEntriesViaStation;\nfbFillStation.xActivateAPLEntriesForMovedCarriers \t\t\t:= xApplicationLoggerEntriesActiveFill;\nfbCloseStation.xEnableActivationOfAPLEntriesForCarriers   \t:= GVL_Project.G_xEnableAplEntriesViaStation;\nfbCloseStation.xActivateAPLEntriesForMovedCarriers \t\t\t:= xApplicationLoggerEntriesActiveClose;\nfbUnloadStation.xEnableActivationOfAPLEntriesForCarriers  \t:= GVL_Project.G_xEnableAplEntriesViaStation;\nfbUnloadStation.xActivateAPLEntriesForMovedCarriers \t\t\t:= xApplicationLoggerEntriesActiveUnLoad;\n\n\nfbRTrigStop(CLK := i_xStop);\nIF fbRTrigStop.Q THEN\n\tStoreCurrentMoveCmds();\n\tStopAllCarriers();\n\tResetTriggerAndStates();\nEND_IF\n\nReportError();\n\nq_xError \t\t:= xError;\nq_sEtResult \t:= sEtResult;\nq_sResultMsg \t:= sResultMsg;\n\n\n\n\n//-- force green color of button\t\nIF uiButtonColor > 0 THEN\n\tfbButtonTimer(IN := TRUE, PT := T#500MS);\n\tIF fbButtonTimer.Q THEN\n\t\tuiButtonColor := 0;\n\t\tfbButtonTimer(IN := FALSE);\n\tEND_IF\nEND_IF","LineInfoPersistence":"(string)1125828b-e591-4529-ade6-5e855ba6ecb2_Impl_LineIds"}}}},"Interface":{"@Type":"a9ed5b7e","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)PROGRAM SR_PosAndSync\nVAR_INPUT\n\ti_etTraceSource  : ET_TraceSource;\n\ti_ifMultiCarrier : MCR.IF_Multicarrier;\n\ti_xStart\t \t : BOOL;\n\ti_xStop\t \t\t : BOOL;\nEND_VAR\n\nVAR_OUTPUT\n\tq_xError\t: BOOL;\n\tq_sEtResult \t: STRING(255);\n\tq_sResultMsg \t: String(255);\nEND_VAR\nVAR\n\tfbCloseStation \t\t\t\t\t\t: FB_PosAndSyncClose;\n\tfbFillStation\t\t\t\t\t\t: FB_PosAndSyncFill;\n\tfbLoadStation \t\t\t\t\t\t: FB_PosAndSyncLoad;\n\tfbUnloadStation \t\t\t\t\t: FB_PosAndSyncUnLoad;\n\txStartLoad\t\t\t\t\t\t\t: BOOL;\n\txStartFill\t\t\t\t\t\t\t: BOOL;\n\txStartClose\t\t\t\t\t\t\t: BOOL;\n\txStartUnload\t\t\t\t\t\t: BOOL;\n\tdiTimer1\t\t\t\t\t\t\t: DINT;\n\tdiTimer2\t\t\t\t\t\t\t: DINT;\n\tdiTimer3\t\t\t\t\t\t\t: DINT;\n\tdiTimer4\t\t\t\t\t\t\t: DINT;\t\n\tiStateMoveAllCarriersInFirstStation\t: INT;\n\txStartTimer\t\t\t\t\t\t\t: BOOL;\n\tiStateLogic\t\t\t\t\t\t\t: INT;\n\txMoveAllCarriersInFirstStationIsDone: BOOL;\n\txMoveWarmStartIsDone\t\t\t\t: BOOL;\n\txError\t\t\t\t\t\t\t\t: BOOL;\n\tudiLoop\t\t\t\t\t\t\t\t: UDINT;\n\tetResultMCR\t\t\t\t\t\t\t: MCR.ET_Result;\n\tetResultMCRS\t\t\t\t\t\t: MCRS.ET_Result;\n\tsResultMsg\t\t\t\t\t\t\t: STRING(255);\n\tsEtResult\t\t\t\t\t\t\t: STRING(255);\n\taudiOrderOfCarrier\t\t\t\t\t: ARRAY [1..MCR.GPL.Gc_udiMaxNumberOfCarriers] OF UDINT;\n\tfbLoggerPoint\t\t\t\t\t\t: APL.FB_LoggerPoint;\n\tudiNumberOfCarrier\t\t\t\t\t: UDINT;\n\n\txApplicationLoggerEntriesActiveLoad: BOOL;\n\txApplicationLoggerEntriesActiveFill: BOOL;\n\txApplicationLoggerEntriesActiveClose: BOOL;\n\txApplicationLoggerEntriesActiveUnLoad: BOOL;\n\t\n\tlrRefMinGapToCarrierInFront\t\t\t: LREAL := 10;\n\tlrRefMinGapToCarrierBehind\t\t\t: LREAL := 0;\n\n\txErrorStored: BOOL;\n\txWaitTimer1: BOOL;\n\txWaitTimer2: BOOL;\n\txWaitTimer3: BOOL;\n\txWaitTimer4: BOOL;\n\tdiWaitTimer1: DINT := 800;\n\tdiWaitTimer2: DINT := 1500;\n\tdiWaitTimer3: DINT := 400;\n\tdiWaitTimer4: DINT := 500;\n\t\n\tfbRTrigStop \t\t\t\t\t\t: R_Trig;\n\n\tstProductDimensions\t\t\t \t\t: MCR.ST_Dimensions \t:= (lrLength := 100, lrWidth := 50, lrHeight := 75);\n\tstProductOffset\t\t\t\t \t\t: SE_MATH.ST_Vector3D \t:= (lrX := 50, lrY := 0, lrZ := 50); \n\tstToolPivotPointOffset\t\t\t\t: PDL.ST_Vector3D;\n\n\tuiButtonColor\t\t\t\t\t\t: UINT;\n\tfbButtonTimer\t\t\t\t\t\t: TON;\n\t\n\tastCarrierData\t\t\t\t\t\t: ARRAY [1..MCR.GPL.Gc_udiMaxNumbe