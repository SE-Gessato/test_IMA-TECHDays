{"payload":{"meta":{"Graph":{"@Type":"81297157","@Value":{"Guid":"(Guid)a8449d28-e626-4ccc-a79e-12880020f956","ParentGuid":"(Guid)a8736412-820a-458d-a88c-f60de058aebb","Name":"(string)SetCurrentModule","Properties":{"@Type":"2c41fa04:IDictionary","@Value":{}},"TypeGuid":"(Guid)8ac092e5-3128-4e26-9e7e-11016c6684f2","EmbeddedTypeGuids":{"@Type":"[Guid]","@Value":["(Guid)3b83b776-fb25-43b8-99f2-3c507c9143fc"]}}},"TypeInfos":{"2c41fa04":"{2c41fa04-1834-41c1-816e-303c7aa2c05b}","81297157":"{81297157-7ec9-45ce-845e-84cab2b88ade}","Guid":"System.Guid","string":"System.String"}},"object":{"Graph":{"@Type":"8ac092e5","@Value":{"Implementation":{"@Type":"3b83b776","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)(*\n\thandles all requests coming from the special vis controllers\n\n\tsets the current module by modifying aiHierachy\n\talso sets the jog target\n\ninputs:\n\tdiSetAsCurrentModule - if <> 0 contains the module ID of the module to set as current module\n\nin_out:\n\taudiHierarchy\t\n\tudiCurrentLevel\n\nflags:\n\t\n\n*)\nIF(udiSetAsCurrentModule > 0) THEN\n\tudiTmp2 := 1;\n\tWHILE(udiSetAsCurrentModule > udiTmp2 * 100) DO\n\t\tudiTmp2 := udiTmp2 * 100;\n\tEND_WHILE\n\n\tudiCurrentLevel := 0;\n\tWHILE(udiTmp2 > 0) DO\n\t\tudiCurrentLevel := udiCurrentLevel + 1;\n\t\taudiHierarchy[udiCurrentLevel] := udiSetAsCurrentModule / udiTmp2;\n\t\tudiSetAsCurrentModule := udiSetAsCurrentModule MOD udiTmp2;\n\t\tudiTmp2 := udiTmp2 / 100;\n\tEND_WHILE\n\n\tIF(udiCurrentLevel < 1) THEN\n\t\tudiCurrentLevel := 1;\n\tEND_IF\n\n\tudiSetAsCurrentModule := 0;\n\txUpdateData := TRUE;\nEND_IF\n\nIF(udiSetAsJogTarget <> 0) THEN\n\tG_udiManualModuleSelect\t\t:= udiSetAsJogTarget;\n//\tG_udiManualElementSelect\t:= udiSetAsJogTargetNum;\n\tudiSetAsJogTarget := 0;\n//\tudiSetAsJogTargetNum := 0;\nEND_IF","LineInfoPersistence":"(string)a8449d28-e626-4ccc-a79e-12880020f956_Impl_LineIds"}}}},"UniqueIdGenerator":"(string)90"}},"TypeInfos":{"3b83b776":"{3b83b776-fb25-43b8-99f2-3c507c9143fc}","8ac092e5":"{8ac092e5-3128-4e26-9e7e-11016c6684f2}","f3878285":"{f3878285-8e4f-490b-bb1b-9acbb7eb04db}","string":"System.String"}}},"FormatVersion":"1.0"}